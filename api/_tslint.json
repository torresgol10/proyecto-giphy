{
    "defaultSeverity": "error",
    "extends": [
        "tslint:recommended"
    ],
    "jsRules": {},
    "rules": {
        "trailing-comma": [ false ],
        "no-console": [false, "log", "error"],
        "no-shadowed-variable": [false],
        "ban-types": [false],
        "object-literal-key-quotes": [false, "always"],
        "object-literal-shorthand": false
    },
    "rulesDirectory": []
    
}
